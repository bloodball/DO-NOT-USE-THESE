-- Scripts:

local function PRZGA_fake_script() -- Frame.LocalScript 
	local script = Instance.new('LocalScript', Frame)

	local noobgui = script.Parent.loading
	wait(2)
	noobgui.Text = "Loading.."
	wait(.50)
	noobgui.Text = "Loading..."
	wait(.50)
	noobgui.Text = "Loading.."
	wait(.50)
	noobgui.Text = "Loading."
	wait(.50)
	noobgui.Text = "Loading.."
	wait(.50)
	noobgui.Text = "Loading..."
	wait(.50)
	noobgui.Text = "Loaded"
	wait(.50)
	noobgui.Text = "Welcome " ..game.Players.LocalPlayer.Name.. " !"
	
	wait(1);
	noobgui.Visible = false
	script.Parent.TextLabel.Visible = false
	
	local ts = game:GetService("TweenService")
	local tweeninfo = TweenInfo.new(1,Enum.EasingStyle.Sine,Enum.EasingDirection.Out)
	local properties = {
		Rotation = 360,
	
	}
	local create = ts:Create(script.Parent,tweeninfo,properties)
	
	
	create:Play()
	
	wait(1);
	
	local a = TweenInfo.new(1,Enum.EasingStyle.Sine,Enum.EasingDirection.Out)
	
	
	local prop2 = {
		Rotation = 180,
		Position = UDim2.new(0.511, 0,0.577, 0),
	
		Size = UDim2.new(0, 783,0, 428)
	};
	wait(1)
	local reate = ts:Create(script.Parent, a, prop2);
	
	reate:Play();
	wait(2)
	script.Parent.Visible = false
	
end
coroutine.wrap(PRZGA_fake_script)()






wait(1)











