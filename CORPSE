local function IJPOKQG_fake_script() -- _1norecoil.LocalScript 
	local script = Instance.new('LocalScript', _1norecoil)

	
	local Workspace = game:GetService("Workspace")
	local Cam = Workspace.CurrentCamera
	local NoRecoilEnabled = false
	local prev = Cam.CFrame
	
	Cam:GetPropertyChangedSignal("CFrame"):Connect(function()
		if NoRecoilEnabled == true and (prev.Position - Cam.CFrame.Position).Magnitude < 0.18 then
			Cam.CFrame = prev 
		end
		prev = Cam.CFrame
	end)
	
	
	script.Parent.MouseButton1Click:connect(function()
		if NoRecoilEnabled == true then
			NoRecoilEnabled = false
	
			game:GetService("StarterGui"):SetCore("SendNotification",{
				Title = "Corpse";
				Text = "No Recoil Disabled";
				Icon = "rbxassetid://5878740344"; -- the image if u want. 
			})
		else
			NoRecoilEnabled = true
	
			game:GetService("StarterGui"):SetCore("SendNotification",{
				Title = "Corpse";
				Text = "No Recoil Enabled";
				Icon = "rbxassetid://5878740344"; -- the image if u want. 
			})
		end
	end)
end
