local function KYFL_fake_script() -- _1tornado.LocalScript 
	local script = Instance.new('LocalScript', _1tornado)

	script.Parent.MouseButton1Click:connect(function()
	
		Mouse = game.Players.LocalPlayer:GetMouse()
		game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(-277.593262, 21.75, -236.037613)
		game.Players.LocalPlayer.Character:WaitForChild("[Knife]") 
		LocalPlayer.Character:FindFirstChildOfClass("Humanoid"):UnequipTools()
	
		local star_cframes = {
			CFrame.new(Vector3.new(0, 0, 4)) * CFrame.Angles(math.rad(0), math.rad(0), math.rad(0)), 
			CFrame.new(Vector3.new(0, 0, 0)) * CFrame.Angles(math.rad(0), math.rad(0), math.rad(0)), 
		}
	
		for i, v in pairs(star_cframes) do
			if LocalPlayer.Backpack:FindFirstChild("[Knife]") and LocalPlayer.Backpack["[Knife]"].ClassName == "Tool" then
				LocalPlayer.Backpack["[Knife]"].Grip = v
				LocalPlayer.Backpack["[Knife]"].Parent = LocalPlayer.Character
			end
		end
	
	
		game.Players.LocalPlayer.Character:WaitForChild("[Knife]") 
	
		spin = true
		local spinSpeed = 50 --change speed
		local speaker = game.Players.LocalPlayer
		for i,v in pairs(speaker.Character.HumanoidRootPart:GetChildren()) do
			if v.Name == "Spinning" then
				v:Destroy()
			end
		end
		local Spin = Instance.new("BodyAngularVelocity", game.Players.LocalPlayer.Character.HumanoidRootPart)
		Spin.Name = "Spinning"
		Spin.MaxTorque = Vector3.new(0, math.huge, 0)
		Spin.AngularVelocity = Vector3.new(0,spinSpeed,0)
		wait(0.6)
	
		Mouse.KeyDown:Connect(function(key)
			if key == "t" then
				speaker.Character.HumanoidRootPart.Spinning:Destroy()
			end
		end)
		game.StarterGui:SetCore("SendNotification", {
			Title = "Corpse"; -- the title (ofc)
			Text = "Press t to stop"; -- what the text says (ofc)
			Duration = 5; -- how long the notification should in secounds
			Icon = "rbxassetid://5878740344";
		}) 
	end)
end
