local function BLTLPY_fake_script() -- _1autostomp.LocalScript 
	local script = Instance.new('LocalScript', _1autostomp)

	script.Parent.MouseButton1Click:connect(function()
		game.StarterGui:SetCore("SendNotification", {
			Title = "Corpse"; -- the title (ofc)
			Text = "Press Z to stop/start"; -- what the text says (ofc)
			Duration = 5; -- how long the notification should in secounds
			Icon = "rbxassetid://5878740344";
		}) 
		Toggle = false
	
		while wait(.5) do
			local mouse = game.Players.LocalPlayer:GetMouse()
	
			if Toggle == false then
				game.ReplicatedStorage.MainEvent:FireServer("Stomp")
				mouse.KeyDown:connect(function(k)
					if k == "z" then
						if Toggle == false then 
							Toggle = true
							print('Off')
	
						elseif Toggle == true then 
							print('On')
							Toggle = false
	
						end
					end
				end)
			end
		end
	end)
end
