TextLabel_2.Position = UDim2.new(0.705478907, 0, 0.0711431727, 0)
TextLabel_2.Size = UDim2.new(0, 196, 0, 20)
TextLabel_2.Font = Enum.Font.Cartoon
TextLabel_2.Text = "Made By i am a corpse#6924"
TextLabel_2.TextColor3 = Color3.fromRGB(255, 255, 255)
TextLabel_2.TextSize = 13.000
TextLabel_2.TextStrokeColor3 = Color3.fromRGB(255, 255, 255)

