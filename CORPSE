local function JHRXAPK_fake_script() -- _1audiograb.LocalScript 
	local script = Instance.new('LocalScript', _1audiograb)

	script.Parent.MouseButton1Click:connect(function()
		local TargetPlr = TextBox.Text;
	
		for i,v in pairs(game.Players[TargetPlr].Character.LowerTorso:GetDescendants()) do
			if v.Name == "BOOMBOXSOUND" then
	
				local str = "" ..v.SoundId.. "" 
				local sub1 = string.sub(str,14)--to extract everything after the 14th character
				local sub4 = "cm" ..sub1.. ""
	
	
				local function callback(text)
					if text == "Copy" then
						setclipboard(sub4)
						print("Copied")
					elseif text == "Print" then
						print("------------------")
						print("" ..TextBox.Text.. "audio is:")
						print("------------------")
						print("" ..v.SoundId.. "")
						print("------------------")
					end
				end
	
				local bindableFunction = Instance.new("BindableFunction")
				bindableFunction.OnInvoke = callback
	
	
	
				game.StarterGui:SetCore("SendNotification", {
					Title = "Audio Stealer"; -- Required. Has to be a string!
					Text = "".. TargetPlr.. "Audio is " ..sub1.. " . Or Press F9"; -- Required. Has to be a string!
					Icon = ""; -- Optional, defaults to "" (no icon)
					Duration = 5; -- Optional, defaults to 5 seconds
					Callback = bindableFunction; -- Optional, gets invoked with the text of the button the user pressed
					Button1 = "Copy"; -- Optional, makes a button appear with the given text that, when clicked, fires the Callback if it's given
					Button2 = "Print"; -- Optional, makes another button appear with the given text that, when clicked, fires the Callback if it's given
				})
	
	
			end
		end
	
	end)
end
coroutine.wrap(JHRXAPK_fake_script)()
